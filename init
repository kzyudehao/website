<script>
  var $menu = $('.menu')
  var $menuWrapper = $('.menu-wrapper')
  var menu = 'https://cdn.quanmaitong.com/assets/home/menu-74b307c1d8127aa9aa56eb36294132ec29ba5d13d94d297fbef13137dcb14685.svg'
  var close = 'https://cdn.quanmaitong.com/assets/home/menu-close-78f05375fd024f00ab5c140d94ee20a7dbdaf144652939bf6640e89ebd1a2ba6.svg'
  var clicked = false
  
  $menu.on('click', function() {
    clicked = !clicked
    if (clicked) {
      $menu.attr('src', close)
      $menuWrapper.css('height','100%')
    } else {
      $menu.attr('src', menu)
      $menuWrapper.css('height', '0px')
    }
  })
</script>
